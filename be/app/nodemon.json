{
  "restartable": "rs",
  "ignore": [".git", "node_modules/**/node_modules"],
  "delay": 500,
  "verbose": false,
  "events": {
    "restart": "echo 'App restarted due to:\n'$FILENAME'\"'"
  },
  "signal": "SIGKILL",
  "watch": ["./**/*.ts"],
  "env": {
    "DB_URL": "postgresql://root:root@localhost:5432/movietonedb",
    "PORT": "4000"
  }
}
